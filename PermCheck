import java.util.Arrays;

class Solution {
    public int solution(int[] A) {
        int result = 1;
        Arrays.sort(A);
        for(int i=0;i<A.length;i++) {
            if(A[i] != i+1) {
                result = 0;
                break;
            } 
        }
        return result;
    }
}
